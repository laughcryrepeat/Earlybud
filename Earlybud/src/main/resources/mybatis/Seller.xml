<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.earlybud.model.Seller">
  	<resultMap type="Seller" id="sellerMap"><!-- dto mapping -->
  		<result property="email" column="email"/>
  		<result property="image" column="ITEM_CODE"/>
  		<result property="seller_profile" column="seller_profile"/>
  		<result property="seller_loc" column="seller_loc"/>
  		<result property="seller_account" column="seller_account"/>
  	</resultMap>
  	
  	<insert id="insertItem3" parameterType="String">
  	update ITEM set
  	image = #{image}, seller_profile = #{seller_profile}, seller_loc = #{seller_loc}, seller_account=#{seller_account}
  	where email = #{email}
  	</insert>
  </mapper>