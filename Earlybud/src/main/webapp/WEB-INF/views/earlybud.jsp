$(window).scroll(function(){
  var $idNewHeader = $('#newHeader');
  var $newHeaderWrap = $('.new-header-wrap');
  if($(window).scrollTop() > 50){
    $newHeaderWrap.css('position','fixed');
    $newHeaderWrap.css('top','0');
  }else{
    $newHeaderWrap.css('position','relative');
    $newHeaderWrap.css('top','0');
    $idNewHeader.removeClass('active');
    $idNewHeader.removeClass('activeM');
    $('#newHeader .gnb .gnb-info').removeClass('active');
    $('#mainContainer, #newContainer, #container').css('padding-top','0');
  }
});

function selectRankingTab(num){
  var tabNum = num-1;
  $('#rankingSection .helpinfo').removeClass('invest').removeClass('reward').removeClass('coming');
  if(num == 1){
    $('#rankingSection .helpinfo').addClass('invest');
  }else if(num == 2){
    $('#rankingSection .helpinfo').addClass('reward');
  }else{
    $('#rankingSection .helpinfo').addClass('coming');
  }
  $('.ranking-content').hide();
  $('#rankContent'+num).show();
  $('.ranking-tab li').removeClass('active');
  $('.ranking-tab li:eq('+tabNum+')').addClass('active');
}
function goInvestMain(cateOfBsns){
  location.href = "/web/winvest/main?_refer_section_st=CTI_0&cateOfBsns="+cateOfBsns;
}
function goImproveInvestMain(mainCategory){
	location.href = "/web/winvest/" + mainCategory;
}
function goRewardMain(custValueCode){

  var url = "/web/wreward/";
  var referSectionSt="?_refer_section_st=CTR_0";

  if(custValueCode === '312') {
    url+= "donation"+referSectionSt;
  } else {
    url+= "category/"+custValueCode+referSectionSt;
  }

  location.href = url;
}
function scrollToTop(){
  $('html, body').animate({scrollTop : 0},600);
  return false;
}

